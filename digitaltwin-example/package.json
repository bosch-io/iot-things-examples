{
  "name": "digitaltwin-example",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run serve",
    "build": "npm run build-ts && npm run tslint",
    "serve": "node dist",
    "watch-node": "nodemon dist",
    "watch": "concurrently -k -p \"[{name}]\" -n \"typeScript,node frontend\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "test": "jest --forceExit --coverage --verbose",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"typeScript,node frontend\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.11.1",
    "@types/node": "~9.3.0",
    "@types/request-promise-native": "^1.0.15",
    "@types/ws": "~3.2.1",
    "ajv": "~6.1.1",
    "express": "^4.16.3",
    "https-proxy-agent": "~2.2.1",
    "request": "^2.88.0",
    "request-promise-native": "~1.0.5",
    "sha.js": "^2.4.11",
    "ws": "~4.0.0",
    "sshpk": "~1.14.2",
    "lodash": "4.17.11"
  },
  "devDependencies": {
    "@types/jest": "~23.3.9",
    "@types/supertest": "^2.0.5",
    "concurrently": "~3.5.1",
    "jest": "^23.6.0",
    "nodemon": "^1.14.12",
    "supertest": "~3.0.0",
    "tslint": "~5.9.1",
    "tslint-config-standard": "~7.0.0",
    "typescript": "^2.7.2"
  },
  "engines": {
    "npm": "^5.4.0",
    "node": "^8.8.0"
  }
}
